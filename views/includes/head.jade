head
    title Спискок популярных друзей
    link(rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css")
    link(rel="stylesheet" href="/css/style.css")
    script(src="http://api-maps.yandex.ru/2.1/?lang=ru_RU")
    script(src="/js/script.js")
    style.
        body{
            padding: 24px 0;
        }
        .ya-container{
            min-height: 400px;
            margin: 10px;
            padding: 0;
            -webkit-box-shadow: 1px 1px 2px 1px rgba(22,22,22,.5);
            box-shadow: 1px 1px 2px 1px rgba(22,22,22,.5);
            .form-inline{
                padding: 6px 6px 0;
            }
        }
        .map-content{
            position:relative;
        }
        .YaMap {
            width:99.9%;
            height:400px;
            padding:8px 0 0;
        }
        .aim {
            position: absolute;
            width: 32px;
            height: 32px;
            z-index: 100;
            background: url(data:image/gif;base64,) no-repeat left center;
        }
        .form-group{
            padding: 10px 10px 0;
        }